{
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "parser": "typescript",
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "printWidth": 130,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": true,
        "explicitTypes": "always",
        "semi": true
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript",
        "semi": true
      }
    }
  ]
}
